{
  "presets": ['react', 'es2015', 'stage-1'],
  "plugins": [
    'add-module-exports',
    ["babel-plugin-styled-components", {
      "ssr": true,
      "displayName": false
    }],
    ["prismjs", {
        "languages": ["javascript", "css", "markup", "ruby", "yaml", "json", "erb", "diff"],
        "plugins": ["line-numbers"],
        "css": true
    }]
  ]
}
